{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Linker",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/linker/main.exe",
            "cwd": "${workspaceFolder}/linker",
            "preLaunchTask": "Build Linker",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "externalConsole": false
        },
